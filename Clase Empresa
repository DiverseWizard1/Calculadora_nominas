package ClasesNominas;

import Sleer1.SLeer1;

/**
 *
 * @author auron
 */
public class Empresa {

    //Datos de la empresa
    private String domicilio;        //Domicilio de la empresa
    private String cif;              //Codigo de identificacion fiscal
    private long ccc;                //Codigo cuenta de cotizacion

    public Empresa(String domi, String cifem, long cccoti) {
        
        domicilio = domi;
        cif = cifem;
        ccc = cccoti;
        
    }

    public void setDomicilio(String d) {
        domicilio = d;
    }

    public String getDomicilio() {

        return domicilio;
    }

    public void setCif(String ci) {
        cif = ci;
    }

    public String getCif() {
        return cif;
    }

    public void setCcc(long c) {
        ccc = c;
    }

    public long getCcc() {
        return ccc;

    }

}
